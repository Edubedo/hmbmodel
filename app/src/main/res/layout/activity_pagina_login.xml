<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".PaginaLogin">

    <TextView
        android:id="@+id/textView"
        android:layout_width="209dp"
        android:layout_height="40dp"
        android:layout_marginStart="31dp"
        android:layout_marginTop="17dp"
        android:layout_marginBottom="7dp"
        android:text="Bienvenid@"
        android:textSize="26sp"
        app:layout_constraintBottom_toTopOf="@+id/textView2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="34dp"
        android:layout_marginEnd="34dp"
        android:layout_marginBottom="40dp"
        android:text="BIENVENIDO A HMB MODEL LOB@, AUUUUUUUUU!!!!! Aqui te pondremos mamad@ en poco tiempo y seras capaz de atraer a tod@s l@s lob@s que quieras, inicia sesion para desarrollar esos musculos AUUUUUUUUUUUUUU"
